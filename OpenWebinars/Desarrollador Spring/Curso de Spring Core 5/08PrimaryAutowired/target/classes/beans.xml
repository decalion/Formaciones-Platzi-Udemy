<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">




	<bean id="emailService"
		class="com.icaballero.app.beans.EmailService" autowire="byType">
		<property name="saludator" ref="saludator"></property>
	</bean>
	
	<bean id="saludator" class="com.icaballero.app.beans.Saludator" primary="true">
		<property name="mensaje" value="Hola mi nombre es Ismael!!!"></property>
	</bean>


	<bean id="englishsaludator" class="com.icaballero.app.beans.Saludator" >
		<property name="mensaje" value="Hola mi nombre es Ismael!!!"></property>
	</bean>



</beans>
